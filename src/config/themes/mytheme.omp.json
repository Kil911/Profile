{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#b8e6ff",
          "foreground": "#000000",
          "properties": {
            "source": "cli"
          },
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "template": "\ufd03\uebc4{{ .Name }}",
          "type": "az"
        },
        {
          "background": "#81b5ff",
          "foreground": "#000000",
          "properties": {
            "source": "pwsh"
          },
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "template": "\ufd03\uebc7{{ .Name }}",
          "type": "az"
        },
        {
          "background": "#0087d8",
          "foreground": "#003544",
          "properties": {
            "display_mode": "context",
            "fetch_virtual_env": true
          },
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "template": "\ue235 {{ .Venv }} ",
          "type": "python"
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#000000",
          "background": "#00ffff",
          "template": "\uE77F {{ .Full }} "
        },
        {
          "type": "go",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#ffffff",
          "background": "#7FD5EA",
          "template": "\uFCD1 {{ .Full }} "
        },
        {
          "type": "java",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#ffffff",
          "background": "#4063D8",
          "template": "\uE738 {{ .Full }}"
        },
        // {
        //   "type": "kubectl",
        //   "style": "diamond",
        //   "leading_diamond": "\uE0B6",
        //   "trailing_diamond": "\uE0B4",
        //   "foreground": "#000000",
        //   "background": "#ebcc34",
        //   "template": "\uFD31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} "
        // },
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "template": "\uE718 {{ .Full }} "
        },
        {
          "type": "rust",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "foreground": "#193549",
          "background": "#99908a",
          "template": "\uE7a8 {{ .Full }} "
        },
        {
          "background": "#f36943",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#33DD2D{{end}}",
            "{{if eq \"Discharging\" .State.String}}#FFCD58{{end}}",
            "{{if eq \"Full\" .State.String}}#0476d0{{end}}"
          ],
          "foreground": "#ffffff",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "template": "{{ if not .Error }}ðŸ”‹{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295",
          "type": "battery"
        },
        {
          "background": "#aa00c0",
          "foreground": "#fff",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "template": "âŒš{{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#ff9639",
          "foreground": "#000000",
          "style": "powerline",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background": "#fff700",
          "foreground": "#003544",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_separator_icon": "/",
            "style": "mixed",
            "mixed_threshold": 4,
            "folder_icon": "ðŸ“‚"
          },
          "style": "powerline",
          "template": "{{ .Path }}",
          "type": "path"
        },
        {
          "background": "#d2ff5e",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#f17c37{{ end }}"
          ],
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "style": "powerline",
          "template": " {{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#33DD2D",
          "background_templates": ["{{ if gt .Code 0 }}#f1184c{{ end }}"],
          "foreground": "#242424",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " \ufc8d ",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#00ffd5",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": "ðŸ•º{{ .UserName }} ",
          "type": "session"
        },

        {
          "type": "shell",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#000000",
          "background": "#ff9639",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "PS"
            }
          }
        },
        {
          "foreground": "#FFD700",
          "properties": {
            "root_icon": "\u26a1"
          },
          "style": "plain",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "foreground": "#f1184c",
          "style": "plain",
          "template": "ðŸš€ ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 2
}
