<?xml version="1.0" encoding="utf-8"?>
<Types>
  <Type>
    <Name>System.Byte[]</Name>
    <Members>
      <ScriptProperty>
        <Name>Hex</Name>
        <GetScriptBlock>
                    $this | % {write-host $([String]::Format("{0:X}",$_)) -nonew}
        </GetScriptBlock>
      </ScriptProperty>
    </Members>
  </Type>
</Types>
